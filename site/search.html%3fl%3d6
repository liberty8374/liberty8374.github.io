                        labels.push($(this).prop('name').replace('label_', ''));
                    });

                    var data = {
                        storyId: '437',
                        labels: labels
                    };

                    $.post('/api/site/labels', data, function(response) {
                        resolve();
                    });
                });
            }
        }).then(function(){
            window.location.reload();
        });

        return false;
    }
</script>

