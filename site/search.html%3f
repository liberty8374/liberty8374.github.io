            function language_change(selected) {
                var lng = selected.value;
                if(lng == undefined)
                    lng = selected;
                //alert(lng);
                $.ajax({
                    type : "POST",
                    url : "/site/language.html",
                    data : "_lang="+lng+"&YII_CSRF_TOKEN=ejVIcEFxSk0OUi43eB0EFzVCMAMeHHgnJV07IDgBJRQpcjoCAj0mIg==",
                    success : function(data){window.location.reload();}
                });
            }
        </script>

