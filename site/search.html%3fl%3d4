<div class="clear-story-wrapper">

<script type="text/javascript">
    function manageLabels(storyId) {

        swal({
            title: 'Управление метками',
            type: 'info',
            allowOutsideClick: false,
            showCancelButton: true,
            cancelButtonText: 'Отмена',
            confirmButtonText: 'Сохранить',
            html: $('#story-labels').html(),
            preConfirm: function(){
                return new Promise(function(resolve) {
                    var labels = []
                    $('.swal2-content input:checked').each(function(){
                        labels.push($(this).prop('name').replace('label_', ''));
                    });

                    var data = {
                        storyId: '6090',
                        labels: labels
                    };

                    $.post('/api/site/labels', data, function(response) {
                        resolve();
                    });
                });
            }
        }).then(function(){
            window.location.reload();
        });

        return false;
    }
</script>

