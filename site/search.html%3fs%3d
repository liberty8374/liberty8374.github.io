<script type="text/javascript">
    var Training = new TrainingMode();
    $( "#search form" ).submit(function( event ) {
      event.preventDefault();
      var search = $(this).find('input.form-control').val();
      if (search) {
          window.location.replace('/site/search.html?s='+search)
      }
    });

