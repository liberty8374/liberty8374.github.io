<script type="text/javascript">jQuery(document).ready(function () {
var showFilter = 1;

if(showFilter) {
    $('.filter-params').show();
}

$('.filter-params-link>a').click( function(){
    $('.filter-params').toggle();
});

$('#filter-top').change(function(){
    var top = $(this).val(),
        optionsHtml = $('#period-month').html();

    if(top == 'week') {
        $('#filter-period').html(optionsHtml);
        $('#filter-period').attr('disabled', true);
    } else if(!top) {
        $('#filter-period').html(optionsHtml);
        $('#filter-period').removeAttr('disabled');
    } else {
        $('#filter-period').html( $('#period-'+top).html() );
        $('#filter-period').find('option[value=""]').remove();
        $('#filter-period').removeAttr('disabled');
    }
    $('#filter-period').trigger('chosen:updated');
});


$('.help-submit .button-default').click(function(event){
    var msg = $('.question_form').val();
    console.log(msg);
    event.preventDefault();
    $.post('/api/site/ticket', {message: msg}, function(data) {
        if(data.ok == true) {
            swal('Ваше сообщение отправлено!');
            $('.question_form').val("");
        } else {
            console.log(data);
            $('.question_form').val("");
        }
    });

});

$('.help-submit .btn-switcher').click(function(event){
    event.preventDefault();
    if (Training.isOff()) {
      Training.on()
    } else {
      Training.off()
    }
});

var SubscribeReminder = {
    timeout: 180,
    now: function() {
        return Date.now()/1000|0;
    },
    setNextAlertIn: function(alertTime) {
        Cookies.set('showSubscribeAlert', alertTime, {expires: 365});
        if(alertTime !== 'false' && alertTime !== false) {
            this.init();
        }
    },
    init: function() {
        var showAlert = Cookies.get('showSubscribeAlert'),
            timer=undefined;
        if(showAlert === undefined || showAlert < this.now()) {
            this.setNextAlertIn(this.now()+this.timeout);
            timer = this.timeout;
        }

        if(showAlert > this.now()) {
            timer = showAlert - this.now();
        }

        if(timer) {
            setTimeout(function() {
               SubscribeReminder.followAlert.call(SubscribeReminder);
            }, timer*1000);
        }
    },
    followAlert: function() {
        that = this;
        swal({
            html: $('#block').html(),
            title: "Подпишись на нас в соцсетях!",
            showCancelButton: true,
            cancelButtonText: "Больше не показывать",
            confirmButtonText: "Позже",
        }).then(function( isConfirm ){
            that.setNextAlertIn( isConfirm ? that.now()+that.timeout : 'false' );
            if (!isConfirm) {
                $.post('/user/disable-social-alert.html', {userId:0});
            }
        });
    }
}

SubscribeReminder.init();
